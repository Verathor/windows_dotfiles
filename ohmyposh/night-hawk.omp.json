{
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "background": "#21c7a8",
          "foreground": "#011627",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}  ",
          "properties": {
            "windows": "\ue62a"
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "#ef5350",
          "foreground": "#ffeb95",
          "template": " \uf292 "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "#82AAFF",
          "foreground": "#011627",
          "template": " {{ .Path }} ",
          "properties": {
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "#addb67",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#e4cf6a{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f78c6c{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }}#c792ea{{ end }}"
          ],
          "foreground": "#011627",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "background": "#575656",
          "foreground": "#d6deeb",
          "leading_diamond": "\ue0d7",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .FormattedMs }}",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "break",
      "segments": [
        {
          "type": "shell",
          "style": "diamond",
          "background": "#d6deeb",
          "foreground": "#011627",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0d6",
          "template": "\uf489  {{ .Name }} "
        },
        {
          "type": "sysinfo",
          "style": "diamond",
          "background": "#8f43f3",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0d6",
          "template": "\ue266 {{ round .PhysicalPercentUsed .Precision }}% "
        },
        {
          "type": "aws",
          "style": "diamond",
          "background": "#565656",
          "foreground": "#faa029",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0d6",
          "template": "\ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} "
        },
        {
          "type": "cmake",
          "style": "diamond",
          "background": "#d2d2d2",
          "foreground": "#01a300",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0d6",
          "template": "<#2829b2>\ue61e</> <#be1818>\ue61d</>  cmake {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "background": "#0e0e0e",
          "foreground": "#0d6da8",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0d6",
          "template": "\ue77f  {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "go",
          "style": "diamond",
          "background": "#ffffff",
          "foreground": "#06aad5",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0d6",
          "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "kubectl",
          "style": "diamond",
          "background": "#316ce4",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0d6",
          "template": "\uf308 {{.Context}} :: {{if .Namespace}}{{.Namespace}}{{else}}default{{end}} "
        },
        {
          "type": "lua",
          "style": "diamond",
          "background": "#ffffff",
          "foreground": "#000081",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0d6",
          "template": "\ue620 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "node",
          "style": "diamond",
          "background": "#303030",
          "foreground": "#3C873A",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0d6",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": "<#cc3a3a>\ue71e</> ",
            "yarn_icon": "<#348cba>\ue6a7</> "
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "background": "#306998",
          "foreground": "#FFE873",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0d6",
          "template": "\ue235  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}"
        },
        {
          "type": "rust",
          "style": "diamond",
          "background": "#ffffff",
          "foreground": "#000000",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0d6",
          "template": "\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "time",
          "style": "diamond",
          "background": "#234d70",
          "foreground": "#d6deeb",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": "\ue641 {{ .CurrentDate | date .Format }}",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#21c7a8",
          "template": "\u2570\u2500"
        },
        {
          "type": "spotify",
          "style": "diamond",
          "background": "#1DB954",
          "foreground": "#011627",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0c6 ",
          "template": " {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} ~ {{ .Track }}{{ end }} ",
          "properties": {
            "playing_icon": "\uf1bc "
          }
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "#22da6e",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ],
          "template": "\ue285\ue285",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "final_space": true,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#d6deeb",
    "template": "\ue285 "
  },
  "version": 3
}
